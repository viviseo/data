    "song_list = []\n",
    "singer_list = []\n",
    "album_list = []\n",
    "like_list = []\n",
    "for i in range(len(tbody.find_elements(By.TAG_NAME,'tr'))):\n",
    "    song_list.append(tbody.find_elements(By.TAG_NAME,'tr')[i].find_element(By.CLASS_NAME,\"ellipsis.rank01\").text)\n",
    "    singer_list.append(tbody.find_elements(By.TAG_NAME,'tr')[i].find_element(By.CLASS_NAME,\"ellipsis.rank02\").text)\n",
    "    album_list.append(tbody.find_elements(By.TAG_NAME,'tr')[i].find_element(By.CLASS_NAME,\"ellipsis.rank03\").text)\n",
    "    like_list.append(tbody.find_elements(By.TAG_NAME,'tr')[i].find_element(By.CLASS_NAME,\"cnt\").text)"
   ]
  },
